{
  "name": "frontwerk",
  "version": "2.1.4",
  "description": "A collection of frontend-friendly scripts for linting, testing and building apps bundled as a CLI",
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "bin": {
    "frontwerk": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tricinel/frontwerk.git"
  },
  "bugs": {
    "url": "https://github.com/tricinel/frontwerk/issues"
  },
  "homepage": "https://github.com/tricinel/frontwerk#readme",
  "author": "Bogdan Lazar",
  "license": "MIT",
  "keywords": [
    "build",
    "frontend",
    "frontwerk",
    "lint",
    "scripts",
    "stylelint",
    "test"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits",
      "pre-push": "npm-run-all --parallel test lint:scripts"
    }
  },
  "scripts": {
    "predevelop": "rimraf dist",
    "develop": "babel --watch --config-file ./babel.config.development.js --extensions '.ts,.js' src --out-dir dist",
    "build": "node dist build",
    "bundle:rollup": "node dist build --bundle",
    "test": "node dist test --no-watch",
    "test:watch": "ts-node src test",
    "test:update": "node dist test --updateSnapshot",
    "test:ci": "node dist test --silent --updateSnapshot",
    "format": "ts-node src format src/**/*.{ts,js}",
    "lint:scripts": "ts-node src lint src/**/*.{ts,js}",
    "lint:css": "ts-node src stylelint",
    "check-all": "npm-run-all format --parallel test lint:scripts",
    "release": "np --no-cleanup",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "files": [
    "dist",
    "babel.js",
    "config.js",
    "eslint.js",
    "jest.js",
    "prettier.js",
    "rollup.js",
    "stylelint.js"
  ],
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    ".tmp",
    "build"
  ],
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "concurrently": "^5.0.0",
    "consola": "^2.10.1",
    "cross-env": "^6.0.3",
    "cross-spawn": "^7.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.6.0",
    "eslint-config-frontwerk": "^1.0.1",
    "eslint-config-frontwerk-react": "^1.0.1",
    "eslint-config-frontwerk-typescript": "^1.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "jest-watch-typeahead": "^0.4.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^6.1.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "ramda": "^0.26.1",
    "read-pkg-up": "^7.0.0",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-multi-input": "^1.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.25.2",
    "stylelint": "^11.1.1",
    "stylelint-a11y": "^1.2.2",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-high-performance-animation": "^1.3.0",
    "stylelint-prettier": "^1.1.1",
    "typescript": "^3.7.2",
    "which": "^2.0.1",
    "yargs-parser": "^16.1.0"
  },
  "devDependencies": {
    "@babel/node": "^7.7.0",
    "@types/jest": "^24.0.22",
    "all-contributors-cli": "^6.10.0",
    "danger": "^9.2.4",
    "husky": "^3.0.9",
    "jest-in-case": "^1.0.2",
    "jest-serializer-path": "^0.1.15",
    "lint-staged": "^9.4.2",
    "np": "^5.1.2",
    "precise-commits": "^1.0.2",
    "rimraf": "^3.0.0",
    "slash": "^3.0.0",
    "ts-node": "^8.5.0"
  }
}
